{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "func-names": ["warn", "as-needed"]
  }
}
